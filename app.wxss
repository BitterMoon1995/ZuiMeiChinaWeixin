swiper-item,
image,
navigator {
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}
/*定义颜色变量和大小变量*/
page {
  --themeColor: #01B0F1;
  font-size: 32rpx;
}
.line {
  width: 100%;
  margin: 10px auto;
  border-top: 1px black solid;
}
.search_row {
  margin-top: 0rpx;
  padding-top: 0;
  padding-left: 15rpx;
  padding-right: 15rpx;
  background-color: white;
  z-index: 100;
  width: 100%;
  height: 78rpx;
  position: fixed;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.search_row input {
  width: 500rpx;
  height: 50rpx;
  padding-left: 20rpx;
  margin-right: 10rpx;
  background-color: white;
  border: #01B0F1 2px solid;
  border-radius: 20rpx;
}
.search_row .cleanIcon {
  position: absolute;
  right: 225rpx;
  height: 50rpx;
  width: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  /*
        一个极小的细节(指卡了两个小时)
        1.什么是z-index？
          z-index 属性设置元素的堆叠顺序。拥有更高堆叠顺序的元素总是会处于堆叠顺序较低的元素的前面。
          不会影响子向父事件冒泡
          周神：但是可以影响兄弟元素的事件顺序★★★
        2.此处作用：优先触发cleanIcon而非input的点击事件，否则cleanIcon永远别想被点击了
     */
  z-index: 101;
}
.search_row .cleanIcon image {
  width: 70%;
}
.search_row button {
  width: 10rpx;
  height: 57rpx;
  padding: 0;
  margin-right: 20rpx!important;
  background-image: linear-gradient(to right, #89f7fe, #66a6ff);
  border-radius: 30rpx;
  font-size: 29rpx;
  color: white;
  display: flex;
  justify-content: center;
  align-items: center;
}
.search_row button image {
  width: 30%;
}
.search_content .search_item {
  background-color: white;
  font-size: 28rpx;
  padding: 15rpx 10rpx;
  border-bottom: 1rpx solid #ccc;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.container {
  width: 100%;
  margin-bottom: 100rpx;
  padding: 0rpx 10rpx 10rpx;
  background-color: white;
  box-sizing: border-box;
  overflow: hidden;
}
.dataDisplay {
  margin-bottom: 30rpx;
}
.confirmBtn {
  width: 260rpx!important;
  text-align: center;
  border-radius: 30rpx;
  margin-bottom: 30rpx;
  background-image: linear-gradient(to right, #89f7fe, #66a6ff);
  color: white;
}
.photoBtn {
  width: 260rpx!important;
  text-align: center;
  border-radius: 30rpx;
  margin-bottom: 30rpx;
  background-image: linear-gradient(to right, #a8edea, #fed6e3);
  color: white;
}
.normalBtn {
  width: 260rpx!important;
  text-align: center;
  border-radius: 30rpx;
  margin-bottom: 30rpx;
  background-color: #777777;
  color: white;
}
.inputCt {
  margin-bottom: 30rpx;
  width: 100%;
  display: flex;
  align-items: center;
}
.inputCt input {
  display: inline-block;
  width: 50%;
  padding-left: 20rpx;
  border: #01B0F1 2px solid;
  border-radius: 20px;
}
.notice {
  display: block;
  margin-top: 300rpx;
}
.block-img {
  margin-top: 30rpx;
  width: 100%;
  overflow: hidden;
